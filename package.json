    "mongoose": "^5.5.13",
    "mongoose-bcrypt": "^1.6.0",
    "pizzicato": "^0.6.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-dropzone": "^10.2.1",
    "react-rangeslider": "^2.2.0",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redis": "^2.8.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
